{
  "name": "llama-pdf-extract",
  "description": "Extract PDF content with Llama Extract for Directus articles",
  "icon": "description",
  "version": "1.0.0",
  "keywords": [
    "directus",
    "directus-extension",
    "directus-extension-api"
  ],
  "type": "module",
  "files": [
    "dist"
  ],
  "directus:extension": {
    "type": "endpoint",
    "path": "dist/index.js",
    "source": "src/index.ts",
    "host": "^11.7.0"
  },
  "scripts": {
    "build": "directus-extension build",
    "dev": "directus-extension build -w --no-minify",
    "link": "directus-extension link",
    "validate": "directus-extension validate"
  },
  "devDependencies": {
    "@directus/extensions-sdk": "13.1.0",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "jsdom": "^26.1.0",
    "multer": "^1.4.5-lts.2",
    "pdf-parse": "^1.1.1"
  }
}
